{
  "name": "encounter-builder-typescript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development parcel ./public/index.html -p 3000 --open",
    "prebuild": "rimraf dist/",
    "build": "cross-env NODE_ENV=production parcel build ./public/index.html --detailed-report --public-url ./",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint './src/**/*.{ts,tsx}'",
    "lint:styles": "stylelint './src/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint:js --fix",
    "type-check": "tsc --noEmit",
    "cache:clear": "rimraf .cache",
    "parse-monsters": "node scripts/parseMonsters.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn type-check && yarn lint-staged"
    }
  },
  "dependencies": {
    "connected-react-router": "^6.5.0",
    "core-js": "^3.1.3",
    "history": "^4.9.0",
    "polished": "^3.4.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-intl": "^3.1.1",
    "react-portal-tooltip": "^2.4.7",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-select": "3.0.4",
    "react-styled-flexboxgrid": "^3.1.0",
    "react-table": "^6.10.0",
    "redux": "^4.0.1",
    "redux-form": "^8.2.3",
    "redux-saga": "^1.0.2",
    "regenerator-runtime": "^0.13.2",
    "styled-components": "^4.2.1",
    "uuid": "^3.3.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/node": "^12.0.4",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-intl": "^3.0.0",
    "@types/react-redux": "^7.0.9",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-select": "^3.0.0",
    "@types/react-table": "^6.8.3",
    "@types/redux": "^3.6.0",
    "@types/redux-form": "^8.1.3",
    "@types/redux-saga": "^0.10.5",
    "@types/styled-components": "^4.1.16",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-preset-nano-react-app": "^0.1.0",
    "cross-env": "^6.0.0",
    "dotenv": "^8.0.0",
    "eslint": "^6.0.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^2.0.1",
    "husky": "^3.0.0",
    "lint-staged": "^9.0.1",
    "node-fetch": "^2.6.0",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.17.1",
    "rimraf": "^3.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "typescript": "^3.4.2",
    "utility-types": "^3.7.0"
  },
  "alias": {
    "public": "./public",
    "pages": "./src/pages",
    "store": "./src/store",
    "styles": "./src/styles",
    "config": "./src/config",
    "shared": "./src/shared"
  }
}
